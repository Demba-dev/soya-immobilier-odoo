<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Séquences pour les Contrats de Location -->
        <record id="seq_soya_rental_contract" model="ir.sequence">
            <field name="name">SOYA Rental Contract</field>
            <field name="code">soya.rental.contract</field>
            <field name="prefix">LOC-</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
        </record>

        <!-- Séquences pour les Contrats de Vente -->
        <record id="seq_soya_sale_contract" model="ir.sequence">
            <field name="name">SOYA Sale Contract</field>
            <field name="code">soya.sale.contract</field>
            <field name="prefix">VEN-</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
        </record>

        <!-- Séquences pour les Avenants -->
        <record id="seq_soya_amendment" model="ir.sequence">
            <field name="name">SOYA Contract Amendment</field>
            <field name="code">soya.contract.amendment</field>
            <field name="prefix">AV-</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
        </record>

        <!-- Séquence pour le Contrat de Base -->
        <record id="seq_soya_base_contract" model="ir.sequence">
            <field name="name">SOYA Base Contract</field>
            <field name="code">soya.base.contract</field>
            <field name="prefix">BASE-</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
        </record>

    </data>
</odoo>
